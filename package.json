{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "homepage": "https://smestach.github.io/parquet-viewer",
  "scripts": {
    "dev": "bun --hot src/index.html",
    "build": "bun build ./src/index.html --outdir=build --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/index.tsx"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.30.0",
    "duckdb-wasm-kit": "^0.1.39",
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "gh-pages": "^6.3.0"
  }
}
